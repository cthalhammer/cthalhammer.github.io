    & :is(:where(h1, h2, h3, h4, h5, h6, th):not(:where([class~="not-prose"],[class~="not-prose"] *))) {
      &:hover {
        @media (hover: hover) {
          &::before {
            content: var(--tw-content);
            color: var(--color-accent);
          }
        }
      }
    }
  }
  .sm\:prose-headings\:before\:content-\[\'\#\'\] {
    @media (width >= 40rem) {
      & :is(:where(h1, h2, h3, h4, h5, h6, th):not(:where([class~="not-prose"],[class~="not-prose"] *))) {
        &::before {
          --tw-content: '#';
          content: var(--tw-content);
        }
      }
    }
  }
  .sm\:prose-th\:before\:content-none {
    @media (width >= 40rem) {
      & :is(:where(th):not(:where([class~="not-prose"],[class~="not-prose"] *))) {
        &::before {
          content: var(--tw-content);
          --tw-content: none;
          content: none;
        }
      }
    }
  }
  .sm\:\[\&_q\]\:col-start-2 {
    @media (width >= 40rem) {
      & q {
        grid-column-start: 2;
      }
    }
  }
  .\[\&\>p\:last-of-type\]\:mb-0 {
    &>p:last-of-type {
      margin-bottom: calc(var(--spacing) * 0);
    }
  }
}
@layer base {
  html {
    color-scheme: light dark;
    accent-color: var(--color-accent);
    scrollbar-gutter: stable;
    &[data-theme="light"] {
      color-scheme: light;
    }
    &[data-theme="dark"] {
      color-scheme: dark;
      --color-global-bg: oklch(23.64% 0.0045 248);
      --color-global-text: oklch(83.54% 0 264);
      --color-link: oklch(70.44% 0.1133 349);
      --color-accent: oklch(70.91% 0.1415 163.7);
      --color-accent-2: oklch(94.66% 0 0);
      --color-quote: oklch(94.8% 0.106 136.49);
    }
  }
  :target {
    scroll-margin-block: 5ex;
  }
  @view-transition {
    navigation: auto;
  }
  :where([data-astro-image]) {
    object-fit: var(--fit);
    object-position: var(--pos);
  }
  [data-astro-image="full-width"] {
    width: 100%;
  }
  [data-astro-image="constrained"] {
    max-width: 100%;
  }
}
